function (self, unitId, unitFrame, envTable, modTable)

    local arenaTargets = envTable.arenaTargets

    unitFrame.arenaLabel:Hide()

    if (Plater.ZoneInstanceType == "arena") then
        local atName = GetUnitName(self.unit, true)

        if arenaTargets[atName] then
            unitFrame.arenaLabel:Show()
            unitFrame.arenalabel.label = arenaTargets[atName]
        end
    end

    if modTable.config.debug then
        unitFrame.arenaLabel:Show()
        if math.random(2) == 1 then
            unitFrame.arenaLabel.label = 1
        else
            unitFrame.arenaLabel.label = 2
        end
    end
end



