function (self, unitId, unitFrame, envTable)
    
    --colors for each time bracket
    envTable.Colors = {
        critical = "red",
        warning = "yellow",
        okay = "white",
    }
    
    --time amount to enter in warning or critical state
    envTable.Timers = {
        critical = 4.9,
        warning = 8,
    }
    
end
